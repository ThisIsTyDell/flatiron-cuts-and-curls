<%= form_for(service) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :title %><br>
  <%= f.label :price %>
  <%= f.number_field :price %><br>
  <%= f.label :'Select Category' %>
  <%= f.collection_select(:category_id, Category.all, :id, :title, {include_blank: true}) %><br>
  <%= f.submit %>
<% end %>

<%= link_to 'Cancel', services_path %>